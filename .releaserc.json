{
  "plugins": [["@semantic-release/npm", { "npmPublish": false }], "@semantic-release/github"],
  "prepare": [
    "@semantic-release/npm",
    {
      "path": "@semantic-release/git",
      "assets": ["package.json"],
      "message": "chore(release): ${nextRelease.version}\n\n${nextRelease.notes}"
    }
  ]
}
